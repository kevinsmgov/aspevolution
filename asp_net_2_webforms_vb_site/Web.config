<?xml version="1.0"?>
<configuration>
  <connectionStrings>
    <add name="NorthwindConnectionString" connectionString="Provider=Microsoft.Jet.OLEDB.4.0;Data Source=|DataDirectory|\Northwind.mdb;Persist Security Info=True"
      providerName="System.Data.OleDb" />
    <add name="NorthwindConnectionStringDatashape" connectionString="Provider=MSDataShape;Data Provider=Microsoft.Jet.OLEDB.4.0;Data Source=|DataDirectory|\Northwind.mdb;Persist Security Info=True"
      providerName="System.Data.OleDb" />
    <add name="NorthwindSqlConnectionString" connectionString="Data Source=(LocalDB)\MSSQLLocalDB;AttachDbFilename=|DataDirectory|\Northwind.mdf;Integrated Security=True"
      providerName="System.Data.SqlClient" />
  </connectionStrings>
  <system.web>
    <compilation debug="true" defaultLanguage="vb"/>
  </system.web>
</configuration>